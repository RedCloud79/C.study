## 바뀌지 않는 문자열 형태의 리스트 받아서 출력
@'*'의 의미는 포인터 = 화살표(지시자)라는 의미
#if 1
#include <stdio.h>
#include <math.h>


int main(void) {
	const char* eggs[4] = {"egg0", "egg1","egg2", "egg3"};
	for (int i = 0; i < 4; i++) {
		printf("%s\n", eggs[i]);
	}
}
#endif
-------------------------------------------------------------
#if 1
#include <stdio.h>
#include <math.h>
struct Student
{
	char name[20];
	int age;
};
int main(void) {
	struct Student s2 = { "bred", 21};
	struct Student s3 = { "jane", 24 };
	printf("%s\n", s2.name);
	printf("%d\n", s2.age);
	printf("%s\n", s3.name);
	printf("%d\n", s3.age);
}
#endif

## def와 같은 표현의 코드
#if 1
#include <stdio.h>
#include <math.h>
struct Student
{
	char name[20];
	int age;
};
char * getName(struct Student inst) {
	return inst.name;
}
int getAge(struct Student inst) {
	return inst.age;
}
void setAge(struct Student * inst, int newAge) {
	inst->age = newAge; // -> 주소값 변수
}
int main(void) {
	struct Student s2 = { "bred", 21};
	struct Student s3 = { "jane", 24 };
	printf("%s\n", s2.name);
	printf("%d\n", s2.age);
	printf("%s\n", s3.name);
	printf("%d\n", s3.age);

	printf("%s\n", getName(s3));
	printf("%d\n", getAge(s3));
	setAge(&s3, getAge(s3) + 1);
	printf("%d\n", getAge(s3));
}
#endif

























ex_1)
#if 1
#include <stdio.h>
#include <string.h>
void print_ary(int* ary) {
	for (int i = 0; i < 5; i++) {
		printf("%d ", ary[i]);
	}
}
int main(void) {

	int ary[5] = { 10,20,30,40,50 };
	print_ary(ary);

	return 0;
}
#endif
ex_2)
#if 1
#include <stdio.h>
#include <string.h>
void print_ary(int* pa);

int main(void) {

	int ary[5] = { 10,20,30,40,50 };

	print_ary(ary);

	return 0;
}
void print_ary(int* pa) {
	int i;
	for (int i = 0; i < 5; i++) {
		printf("%d ", pa[i]);
	}
}
#endif
--------------------------------------------------------------
#if 1
#include <stdio.h>
#include <string.h>
void print_ary(int* ary1, int ary2) {
	for (int i = 0; i < ary2; i++) printf("%d ", ary1[i]);
}
int main(void) {
	int ary1[5] = { 10,20,30,40,50 };
	int ary2[7] = { 10,20,30,40,50, 60, 70 };
	print_ary(ary1, 5);
	printf("\n");
	print_ary(ary2, 7);
	return 0;
}
#endif
--------------------------------------------------------------
#if 1
#include <stdio.h>
#include <string.h>
void input_ary(double* pa, int size);
double find_max(double* pa, int size);

void input_ary(double*pa, int size) {
	int i;

	printf("%d개의 실수값 입력: ", size);
	for (i = 0; i < size; i++) scanf("%lf", pa + i);
				// 포인터의 형태 ( &pa[i] ) 배열의형태
}
double find_max(double* pa, int size) {
	double max;
	int i;

	max = pa[0];
	for (i = 1; i < size; i++) {
		if (pa[i] > max) max = pa[i];
	}
	return max;
}
int main(void) {
	double ary[5];
	double max;
	int size = sizeof(ary) / sizeof(ary[0]);
	input_ary(ary, size);
	max = find_max(ary, size);
	printf("배열의 최댓값 : %.1lf\n", max);
	return 0;
}
#endif
-----------------------------------------------------------------* 연습문제
#if 1
#include <stdio.h>
#include <string.h>
void func(int *p) { // 정수변수 주소변수 p
	printf("%d ", p[0]);
	p[0] =  2 ;
}
int main(void) {
	int ary[5] = { 1,2,3,4,5 };
	func(ary + 2); // &ary[2]
	// ary <=> ary+0 <=> &ary[0]
	printf("%d\n", ary[0]);
	return 0;
}
#endif
-----------------------------------------------------------------* 연습문제
#if 1
#include <stdio.h>
#include <string.h>
void print_month(int* mp) { // 정수변수 주소변수 p
	int i;

	for (i = 0; i < 12; i++) {
		printf("%5d", mp[i]);
		if (mp[i] % 5 == 0) printf("\n");
	}
	
}
int main(void) {
	int month[12] = { 31,28,31,30,31,30,31,31,30,31,30,31 };
	print_month(month);
	return 0;
}
#endif
---------------------------------------------------------------- * 연습문제
#if 1
#include <stdio.h>
#include <string.h>
void print_nums(int *lotto_nums);
void input_nums(int *lotto_nums);

void input_nums(int *lotto_nums) {
	int i = 0;
	while (i < 6) {
		printf("번호 입력: ");
		scanf("%d", &lotto_nums[i]);
		for (int j = 0; j < i; j++) {
			if (lotto_nums[i] == lotto_nums[j]) {
				printf("같은번호가 있습니다.\n");
				--i;
				break;
			}
		}
		i++;
	}
}

void print_nums(int *lotto_nums) {
	for (int i = 0; i < 6; i++) {
		printf("로또 번호 : %d ", lotto_nums[i]);
	}
	printf("\n");
}

int main(void) {
	int lotto_nums[6] = { 1,2,3,4,5,6 };

	input_nums(lotto_nums);
	print_nums(lotto_nums);
	return 0;
}
#endif
